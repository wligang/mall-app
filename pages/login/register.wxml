<view><view class="logo"><view class="img"><image mode="widthFix" src="{{face}}"></image></view></view><view class="form re"><view class="username"><view data-event-opts="{{[['tap',[['getCode']]]]}}" class="get-code" style="{{'color:'+(getCodeBtnColor)+';'}}" catchtap="__e">{{getCodeText}}</view><input placeholder="请输入手机号" placeholder-style="color: rgba(255,255,255,0.8);" data-event-opts="{{[['input',[['__set_model',['','phoneNumber','$event',[]]]]]]}}" value="{{phoneNumber}}" bindinput="__e"/></view><view class="code"><input placeholder="请输入验证码" placeholder-style="color: rgba(255,255,255,0.8);" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/></view><view class="password"><input placeholder="请输入密码" password="true" placeholder-style="color: rgba(255,255,255,0.8);" data-event-opts="{{[['input',[['__set_model',['','passwd','$event',[]]]]]]}}" value="{{passwd}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['doReg',['$event']]]]]}}" class="btn" bindtap="__e">立即注册</view><view class="res"><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" bindtap="__e">已有账号立即登录</view></view></view></view>